{
  "name": "Simple xdebug",
  "version": "1.0",
  "description": "Xdebug in one click",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "activeTab",
    "cookies"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon48.png",
    "default_title": "Xdebug"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
